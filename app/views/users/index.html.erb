<h1>All Facespace users</h1>
<br/>
<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<ul class="list-group">
			<% @users.each do |user| %>
				<li class="list-group-item">
				<a href="/users/<%= user.id %>"><%= if user.name.nil? then user.email else user.name end %></a>
				</li>
			<% end %>
		</ul>
	</div>
</div>